import 'package:studentapp/data/models/enums.dart';
import 'package:studentapp/data/models/response_entity.dart';
import 'package:studentapp/data/repositories/remote_repository.dart';

import '../../domain/repositories/system_info_repository.dart';

class SystemInfoRepositoryImpl extends RemoteRepository
    implements SystemInfoRepository {
  @override
  Future<ResponseEntity> getSystemInfo() async {
    ResponseEntity response = await RemoteRepository.restApiService.request(
      'api/system-info/system-info/',
      HttpRequestType.get,
    );
    return response;
  }
}
